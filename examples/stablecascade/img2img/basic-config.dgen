#! /usr/bin/env dgenerate --file
#! dgenerate 4.5.1

stabilityai/stable-cascade-prior
--model-type torch-s-cascade
--image-seeds "../../media/beach.jpg"
--variant bf16
--dtype bfloat16
--model-cpu-offload
--second-model-sequential-offload
